pipeline {
    agent any
	
    stages {
        stage('SCM-Checkout') {
            steps {
                git branch: 'main', credentialsId: 'git-credentials', url: 'https://github.com/taakshakti/assessment-ca-test.git'
            }
        }
		stage('Maven-clean') {
            steps {
                echo 'test1'
            }
        }
        stage('Maven-build') {
            steps {
                echo 'test2'
            }
        }
        stage('Maven-deploy') {
            steps {
                echo 'test3'
            }
        }
    }
}
